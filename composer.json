{
    "name": "joomla/preload",
    "type": "joomla-package",
    "description": "Joomla Preload Package",
    "keywords": ["joomla", "framework", "console"],
    "homepage": "https://github.com/joomla-framework/preload",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~7.0",
        "fig/link-util": "~1.0",
        "psr/link": "~1.0"
    },
    "require-dev": {
        "joomla/application": "~2.0",
        "joomla/coding-standards": "~2.0@alpha",
        "joomla/event": "~2.0",
        "joomla/uri": "~2.0",
        "phpunit/phpunit": "~6.3",
        "symfony/web-link": "~3.4|~4.0"
    },
    "suggest": {
        "joomla/application": "To use the PreloadSubscriber event listener, install `~2.0`",
        "joomla/event": "To use the PreloadSubscriber event listener, install `~2.0`",
        "symfony/web-link": "To use the PreloadSubscriber event listener, install `~3.4|~4.0`"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Preload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Preload\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
